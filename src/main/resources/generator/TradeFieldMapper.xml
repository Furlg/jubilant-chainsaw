<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hoop.Mappers.TradeFieldMapper">
  <resultMap id="BaseResultMap" type="hoop.Trade.CommTrade.TradeFieldEntity">
    <result column="InterfaceImpCode" jdbcType="VARCHAR" property="interfaceImpCode" />
    <result column="FiledType" jdbcType="OTHER" property="fieldType" />
    <result column="IsRequire" jdbcType="OTHER" property="isRequire" />
    <result column="CheckField" jdbcType="VARCHAR" property="checkField" />
    <result column="RegularExp" jdbcType="VARCHAR" property="regularExp" />
    <result column="Remark" jdbcType="VARCHAR" property="remark" />
  </resultMap>
  <sql id="Base_Column_List">
     InterfaceImpCode  FiledType IsRequire CheckField RegularExp Remark
  </sql>
  <select id="queryTradeFieldByInterfaceAndScene" parameterType="java.lang.String" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List"/>
    from te_om_manager
    where InterfaceImpCode = #{interfaceImpCode,jdbcType=VARCHAR}
  </select>

</mapper>